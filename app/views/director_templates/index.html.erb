<h1>List of all directors</h1>
<hr>

<table border = "1">

<tr>
  <th>ID</th>
  <th>Image</th>
  <th>Name</th>
  <th>Dob</th>
  <th></th>
</tr>

<% @dir_table.each do |item| %>
<tr>
    <td> <%= item.id %> </td>
    <td> <img src = "<%= item.image %>"></img> </td>
    <td> <%= item.name %> </td>
    <td> <%= item.dob %> </td>
    <td> <a href = "/directors/<%=item.id%>">Show Details</a> </td>
</tr>
<% end %>

</table>
